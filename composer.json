{
  "name": "micronative/sqs",
  "type": "library",
  "description": "AWS SQS Transport (fork and improve from enqueue/sqs)",
  "keywords": [
    "messaging",
    "queue",
    "aws",
    "sqs"
  ],
  "homepage": "https://github.com/micronative/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ngodinhloc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "queue-interop/queue-interop": "^0.7|^0.8",
    "enqueue/dsn": "^0.9",
    "aws/aws-sdk-php": "~3.26"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4.0",
    "enqueue/test": "0.9.x-dev",
    "queue-interop/queue-spec": "^0.6"
  },
  "autoload": {
    "psr-4": {
      "Micronative\\Sqs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Micronative\\Sqs\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
